{
  "name": "crypto-market-monitor",
  "version": "1.0.0",
  "description": "Comprehensive crypto market monitoring with AI-powered analysis",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "server": "node server/index.js",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "echo 'Server-only build completed'",
    "install-client": "echo 'Client install skipped for production'",
    "heroku-postbuild": "echo 'Postbuild completed'",
    "postinstall": "echo 'Postinstall completed'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:api": "jest tests/unit/api.test.js",
    "test:auth": "jest tests/unit/auth.test.js",
    "test:frontend": "jest tests/integration/frontend.test.js"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.7",
    "pg": "^8.11.3",
    "sqlite3": "^5.1.6",
    "stripe": "^14.0.0",
    "web-push": "^3.6.6"
  },
  "devDependencies": {
